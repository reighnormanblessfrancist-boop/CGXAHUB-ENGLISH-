local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
    Name = "CGXA Hub",
    Icon = 0,
    LoadingTitle = "CGXA Hub Project",
    LoadingSubtitle = "by superbadodoy",
    ShowText = "CGXA Hub",
    Theme = "Default",
    ToggleUIKeybind = "K",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = "ConfigFolder",
        FileName = "CGXA Hub"
    },
    Discord = {
        Enabled = false,
        Invite = "noinvitelink",
        RememberJoins = true
    },
    KeySystem = false,
    KeySettings = {
        Title = "Untitled",
        Subtitle = "Key System",
        Note = "No method of obtaining the key is provided",
        FileName = "Key",
        SaveKey = true,
        GrabKeyFromSite = false,
        Key = {"Hello"}
    }
})

-- Home Tab
local HomeTab = Window:CreateTab("Home", "house")
local HomeLabel = HomeTab:CreateLabel("Welcome home!", "house")
local HomeParagraph = HomeTab:CreateParagraph({Title = "Notice", Content = "Scripts might not work due to the executor."})

local TimeDateSection = HomeTab:CreateSection("Time and Date")
local TimeLabel = TimeDateSection:CreateLabel(os.date("%H:%M:%S"))
local DateLabel = TimeDateSection:CreateLabel(os.date("%m-%d-%y"))

local HomeSection = HomeTab:CreateSection("Home")
local DexButton = HomeSection:CreateButton({
    Name = "Dex",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/memeenjoyer43/darkdex/refs/heads/main/script"))()
    end
})
