-- Settings Tab
local SettingsTab = Window:CreateTab("Settings")

local HubColorSection = SettingsTab:CreateSection("Hub Color")
local ColorPicker = HubColorSection:CreateColorPicker({
    Name = "Hub color",
    Color = Color3.fromRGB(255, 255, 255),
    Flag = "Hubcolor1",
    Callback = function(Value)
        Window:ChangeThemeColor(Value)
    end
})
HubColorSection:CreateButton({
    Name = "Change color",
    Callback = function()
        local selectedColor = ColorPicker:UpdateColor()
        Window:ChangeThemeColor(selectedColor)
    end
})

local SimpleColorSection = SettingsTab:CreateSection("Simple color picker")
local RedInput = SimpleColorSection:CreateInput({
    Name = "Red",
    CurrentValue = "",
    PlaceholderText = "Input Red (0-255)",
    RemoveTextAfterFocusLost = false,
    Flag = "InputRed",
    Callback = function() end
})
local GreenInput = SimpleColorSection:CreateInput({
    Name = "Green",
    CurrentValue = "",
    PlaceholderText = "Input Green (0-255)",
    RemoveTextAfterFocusLost = false,
    Flag = "InputGreen",
    Callback = function() end
})
local BlueInput = SimpleColorSection:CreateInput({
    Name = "Blue",
    CurrentValue = "",
    PlaceholderText = "Input Blue (0-255)",
    RemoveTextAfterFocusLost = false,
    Flag = "InputBlue",
    Callback = function() end
})
SimpleColorSection:CreateButton({
    Name = "Change color",
    Callback = function()
        local r = tonumber(RedInput:Get()) or 0
        local g = tonumber(GreenInput:Get()) or 0
        local b = tonumber(BlueInput:Get()) or 0
        Window:ChangeThemeColor(Color3.fromRGB(r, g, b))
    end
})

local SettingsMainSection = SettingsTab:CreateSection("Settings")
SettingsMainSection:CreateDropdown({
    Name = "Language selector",
    Options = {"English", "Filipino", "Spanish", "French", "German", "Japanese"},
    CurrentOption = "English",
    Flag = "LanguageSelector",
    Callback = function() end
})
