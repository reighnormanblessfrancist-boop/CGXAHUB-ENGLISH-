local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
    Name = "CGXA Hub",
    Icon = 0,
    LoadingTitle = "CGXA Hub プロジェクト",
    LoadingSubtitle = "作成者: superbadodoy",
    ShowText = "CGXA Hub",
    Theme = "Default",
    ToggleUIKeybind = "K",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = "ConfigFolder",
        FileName = "CGXA Hub"
    },
    Discord = {
        Enabled = false,
        Invite = "noinvitelink",
        RememberJoins = true
    },
    KeySystem = false,
    KeySettings = {
        Title = "無題",
        Subtitle = "キーシステム",
        Note = "キーを取得する方法は提供されていません",
        FileName = "Key",
        SaveKey = true,
        GrabKeyFromSite = false,
        Key = {"Hello"}
    }
})

-- Home Tab
local HomeTab = Window:CreateTab("ホーム", "house")
local HomeLabel = HomeTab:CreateLabel("ようこそ！", "house")
local HomeParagraph = HomeTab:CreateParagraph({Title = "通知", Content = "スクリプトは実行環境によって動作しない場合があります。"})

local TimeDateSection = HomeTab:CreateSection("時間と日付")
local TimeLabel = TimeDateSection:CreateLabel(os.date("%H:%M:%S"))
local DateLabel = TimeDateSection:CreateLabel(os.date("%m-%d-%y"))

local HomeSection = HomeTab:CreateSection("ホーム")
local DexButton = HomeSection:CreateButton({
    Name = "Dex",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/memeenjoyer43/darkdex/refs/heads/main/script"))()
    end
})
local debUNCButton = HomeSection:CreateButton({
    Name = "debUNC",
    Callback = function()
        loadstring(game:HttpGet("https://github.com/alyssagithub/debUNC/raw/main/debUNC.luau"))()
    end
})

