-- Settings Tab
local SettingsTab = Window:CreateTab("Paramètres")

local HubColorSection = SettingsTab:CreateSection("Couleur du Hub")
local ColorPicker = HubColorSection:CreateColorPicker({
    Name = "Couleur du Hub",
    Color = Color3.fromRGB(255, 255, 255),
    Flag = "Hubcolor1",
    Callback = function(Value)
        Window:ChangeThemeColor(Value)
    end
})
HubColorSection:CreateButton({
    Name = "Changer la couleur",
    Callback = function()
        local selectedColor = ColorPicker:UpdateColor()
        Window:ChangeThemeColor(selectedColor)
    end
})

local SimpleColorSection = SettingsTab:CreateSection("Sélecteur de couleur simple")
local RedInput = SimpleColorSection:CreateInput({
    Name = "Rouge",
    CurrentValue = "",
    PlaceholderText = "Entrez Rouge (0-255)",
    RemoveTextAfterFocusLost = false,
    Flag = "InputRed",
    Callback = function() end
})
local GreenInput = SimpleColorSection:CreateInput({
    Name = "Vert",
    CurrentValue = "",
    PlaceholderText = "Entrez Vert (0-255)",
    RemoveTextAfterFocusLost = false,
    Flag = "InputGreen",
    Callback = function() end
})
local BlueInput = SimpleColorSection:CreateInput({
    Name = "Bleu",
    CurrentValue = "",
    PlaceholderText = "Entrez Bleu (0-255)",
    RemoveTextAfterFocusLost = false,
    Flag = "InputBlue",
    Callback = function() end
})
SimpleColorSection:CreateButton({
    Name = "Changer la couleur",
    Callback = function()
        local r = tonumber(RedInput:Get()) or 0
        local g = tonumber(GreenInput:Get()) or 0
        local b = tonumber(BlueInput:Get()) or 0
        Window:ChangeThemeColor(Color3.fromRGB(r, g, b))
    end
})

local SettingsMainSection = SettingsTab:CreateSection("Paramètres")
SettingsMainSection:CreateDropdown({
    Name = "Sélectionner la langue",
    Options = {"Anglais", "Français", "Espagnol", "Filipino", "Allemand", "Japonais"},
    CurrentOption = "Anglais",
    Flag = "LanguageSelector",
    Callback = function() end
})
