local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
    Name = "CGXA Hub",
    Icon = 0,
    LoadingTitle = "Projet CGXA Hub",
    LoadingSubtitle = "par superbadodoy",
    ShowText = "CGXA Hub",
    Theme = "Default",
    ToggleUIKeybind = "K",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = "ConfigFolder",
        FileName = "CGXA Hub"
    },
    Discord = {
        Enabled = false,
        Invite = "noinvitelink",
        RememberJoins = true
    },
    KeySystem = false,
    KeySettings = {
        Title = "Sans titre",
        Subtitle = "Système de clé",
        Note = "Aucune méthode pour obtenir la clé n'est fournie",
        FileName = "Key",
        SaveKey = true,
        GrabKeyFromSite = false,
        Key = {"Hello"}
    }
})

-- Home Tab
local HomeTab = Window:CreateTab("Accueil", "house")
local HomeLabel = HomeTab:CreateLabel("Bienvenue !", "house")
local HomeParagraph = HomeTab:CreateParagraph({Title = "Avis", Content = "Les scripts peuvent ne pas fonctionner à cause de l'executor."})

local TimeDateSection = HomeTab:CreateSection("Heure et Date")
local TimeLabel = TimeDateSection:CreateLabel(os.date("%H:%M:%S"))
local DateLabel = TimeDateSection:CreateLabel(os.date("%m-%d-%y"))

local HomeSection = HomeTab:CreateSection("Accueil")
local DexButton = HomeSection:CreateButton({
    Name = "Dex",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/memeenjoyer43/darkdex/refs/heads/main/script"))()
    end
})
