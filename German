-- Settings Tab
local SettingsTab = Window:CreateTab("Einstellungen")

local HubColorSection = SettingsTab:CreateSection("Hub-Farbe")
local ColorPicker = HubColorSection:CreateColorPicker({
    Name = "Hub-Farbe",
    Color = Color3.fromRGB(255, 255, 255),
    Flag = "Hubcolor1",
    Callback = function(Value)
        Window:ChangeThemeColor(Value)
    end
})
HubColorSection:CreateButton({
    Name = "Farbe ändern",
    Callback = function()
        local selectedColor = ColorPicker:UpdateColor()
        Window:ChangeThemeColor(selectedColor)
    end
})

local SimpleColorSection = SettingsTab:CreateSection("Einfacher Farbwähler")
local RedInput = SimpleColorSection:CreateInput({
    Name = "Rot",
    CurrentValue = "",
    PlaceholderText = "Geben Sie Rot ein (0-255)",
    RemoveTextAfterFocusLost = false,
    Flag = "InputRed",
    Callback = function() end
})
local GreenInput = SimpleColorSection:CreateInput({
    Name = "Grün",
    CurrentValue = "",
    PlaceholderText = "Geben Sie Grün ein (0-255)",
    RemoveTextAfterFocusLost = false,
    Flag = "InputGreen",
    Callback = function() end
})
local BlueInput = SimpleColorSection:CreateInput({
    Name = "Blau",
    CurrentValue = "",
    PlaceholderText = "Geben Sie Blau ein (0-255)",
    RemoveTextAfterFocusLost = false,
    Flag = "InputBlue",
    Callback = function() end
})
SimpleColorSection:CreateButton({
    Name = "Farbe ändern",
    Callback = function()
        local r = tonumber(RedInput:Get()) or 0
        local g = tonumber(GreenInput:Get()) or 0
        local b = tonumber(BlueInput:Get()) or 0
        Window:ChangeThemeColor(Color3.fromRGB(r, g, b))
    end
})

local SettingsMainSection = SettingsTab:CreateSection("Einstellungen")
SettingsMainSection:CreateDropdown({
    Name = "Sprache auswählen",
    Options = {"Englisch", "Deutsch", "Französisch", "Spanisch", "Filipino", "Japanisch"},
    CurrentOption = "Englisch",
    Flag = "LanguageSelector",
    Callback = function() end
})
