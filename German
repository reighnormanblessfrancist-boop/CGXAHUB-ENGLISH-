local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
    Name = "CGXA Hub",
    Icon = 0,
    LoadingTitle = "CGXA Hub Projekt",
    LoadingSubtitle = "von superbadodoy",
    ShowText = "CGXA Hub",
    Theme = "Default",
    ToggleUIKeybind = "K",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = "ConfigFolder",
        FileName = "CGXA Hub"
    },
    Discord = {
        Enabled = false,
        Invite = "noinvitelink",
        RememberJoins = true
    },
    KeySystem = false,
    KeySettings = {
        Title = "Unbenannt",
        Subtitle = "Schlüsselsystem",
        Note = "Es wird keine Methode zum Erhalten des Schlüssels bereitgestellt",
        FileName = "Key",
        SaveKey = true,
        GrabKeyFromSite = false,
        Key = {"Hello"}
    }
})

-- Home Tab
local HomeTab = Window:CreateTab("Startseite", "house")
local HomeLabel = HomeTab:CreateLabel("Willkommen!", "house")
local HomeParagraph = HomeTab:CreateParagraph({Title = "Hinweis", Content = "Skripte funktionieren möglicherweise nicht aufgrund des Executors."})

local TimeDateSection = HomeTab:CreateSection("Uhrzeit und Datum")
local TimeLabel = TimeDateSection:CreateLabel(os.date("%H:%M:%S"))
local DateLabel = TimeDateSection:CreateLabel(os.date("%m-%d-%y"))

local HomeSection = HomeTab:CreateSection("Startseite")
local DexButton = HomeSection:CreateButton({
    Name = "Dex",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/memeenjoyer43/darkdex/refs/heads/main/script"))()
    end
})
