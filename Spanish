-- Settings Tab
local SettingsTab = Window:CreateTab("Configuración")

local HubColorSection = SettingsTab:CreateSection("Color del Hub")
local ColorPicker = HubColorSection:CreateColorPicker({
    Name = "Color del Hub",
    Color = Color3.fromRGB(255, 255, 255),
    Flag = "Hubcolor1",
    Callback = function(Value)
        Window:ChangeThemeColor(Value)
    end
})
HubColorSection:CreateButton({
    Name = "Cambiar color",
    Callback = function()
        local selectedColor = ColorPicker:UpdateColor()
        Window:ChangeThemeColor(selectedColor)
    end
})

local SimpleColorSection = SettingsTab:CreateSection("Selector de color simple")
local RedInput = SimpleColorSection:CreateInput({
    Name = "Rojo",
    CurrentValue = "",
    PlaceholderText = "Ingrese Rojo (0-255)",
    RemoveTextAfterFocusLost = false,
    Flag = "InputRed",
    Callback = function() end
})
local GreenInput = SimpleColorSection:CreateInput({
    Name = "Verde",
    CurrentValue = "",
    PlaceholderText = "Ingrese Verde (0-255)",
    RemoveTextAfterFocusLost = false,
    Flag = "InputGreen",
    Callback = function() end
})
local BlueInput = SimpleColorSection:CreateInput({
    Name = "Azul",
    CurrentValue = "",
    PlaceholderText = "Ingrese Azul (0-255)",
    RemoveTextAfterFocusLost = false,
    Flag = "InputBlue",
    Callback = function() end
})
SimpleColorSection:CreateButton({
    Name = "Cambiar color",
    Callback = function()
        local r = tonumber(RedInput:Get()) or 0
        local g = tonumber(GreenInput:Get()) or 0
        local b = tonumber(BlueInput:Get()) or 0
        Window:ChangeThemeColor(Color3.fromRGB(r, g, b))
    end
})

local SettingsMainSection = SettingsTab:CreateSection("Configuración")
SettingsMainSection:CreateDropdown({
    Name = "Seleccionar idioma",
    Options = {"Inglés", "Español", "Filipino", "Francés", "Alemán", "Japonés"},
    CurrentOption = "Inglés",
    Flag = "LanguageSelector",
    Callback = function() end
})
